{
  "version": 2,
  "builds": [
    {
      "src": "js/index.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/js/(.*)",
      "destination": "/js/index.mjs"
    }
  ]
}
